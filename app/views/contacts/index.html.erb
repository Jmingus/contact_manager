<p id="notice"><%= notice %></p>

	<div class="row">
    <% @contacts.each do |contact| %>
				<div class="col-md-3"><%= contact.first_name %></div>
	      <div class="col-md-3"><%= contact.last_name %></div>
	      <div class="col-md-3"><%= contact.email %></div>
	      <div class="col-md-3"><%= contact.phone_number %>
	        <%= link_to 'Edit', edit_contact_path(contact) %>
	        <%= link_to 'Destroy', contact, method: :delete, data: { confirm: 'Are you sure?' } %>
				</div>
    <% end %>
	</div>


<br>

